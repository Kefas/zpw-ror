<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
  <tr>
    <th>Artist</th>
    <th>Decription</th>
    <th>Price low</th>
    <th>Price high</th>
    <th>Event date</th>
    <th>Sold tickets</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @events.each do |event| %>
      <tr>
        <td><%= event.artist %></td>
        <td><%= event.description %></td>
        <td><%= event.price_low %></td>
        <td><%= event.price_high %></td>
        <td><%= event.event_date %></td>
        <td><%= event.tickets.size %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>



